		forcelabels=true,
		nodesep=2.0,
		splines=ortho
	];
	n1	[label=x,
		shape=box];
	n5	[label="+"];
	n1 -> n5	[xlabel="S_1 "];
	n6	[label="SUB(x,y)",
		shape=box];
	n5 -> n6	[xlabel="S_3 "];
	n2	[label=y,
		shape=box];
	n4	[label="*"];
	n2 -> n4	[xlabel="S_2 "];
	n4 -> n5;
	n3	[label=-1,
		shape=box];
	n3 -> n4;
